<?xml version="1.0" encoding="UTF-8"?>
<DuplicateRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionOnInsert>Block</actionOnInsert>
    <actionOnUpdate>Block</actionOnUpdate>
    <alertText>Duplicate course code detected. Course codes must be unique.</alertText>
    <description>Prevents duplicate courses based on course code</description>
    <duplicateRuleFilter>
        <booleanFilter>1</booleanFilter>
        <duplicateRuleFilterItems>
            <field>Course_Code__c</field>
            <operation>notEqual</operation>
            <value></value>
        </duplicateRuleFilterItems>
    </duplicateRuleFilter>
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>Course__c</matchRuleSObjectType>
        <matchingRule>Course_Code_Match_Rule</matchingRule>
        <objectMapping>
            <inputObject>Course__c</inputObject>
            <mappingFields>
                <inputField>Course_Code__c</inputField>
                <outputField>Course_Code__c</outputField>
            </mappingFields>
            <outputObject>Course__c</outputObject>
        </objectMapping>
    </duplicateRuleMatchRules>
    <isActive>true</isActive>
    <masterLabel>Course Code Duplicate Rule</masterLabel>
    <securityOption>EnforceSharingRules</securityOption>
    <sObjectType>Course__c</sObjectType>
    <sortOrder>1</sortOrder>
</DuplicateRule>
