<?xml version="1.0" encoding="UTF-8"?>
<DuplicateRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionOnInsert>Allow</actionOnInsert>
    <actionOnUpdate>Allow</actionOnUpdate>
    <alertText>You are creating a duplicate student record. A student with this email already exists.</alertText>
    <description>Prevents duplicate students based on email address</description>
    <duplicateRuleFilter>
        <booleanFilter>1</booleanFilter>
        <duplicateRuleFilterItems>
            <field>Email__c</field>
            <operation>notEqual</operation>
            <value></value>
        </duplicateRuleFilterItems>
    </duplicateRuleFilter>
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>Student__c</matchRuleSObjectType>
        <matchingRule>Student_Email_Match_Rule</matchingRule>
        <objectMapping>
            <inputObject>Student__c</inputObject>
            <mappingFields>
                <inputField>Email__c</inputField>
                <outputField>Email__c</outputField>
            </mappingFields>
            <outputObject>Student__c</outputObject>
        </objectMapping>
    </duplicateRuleMatchRules>
    <isActive>true</isActive>
    <masterLabel>Student Email Duplicate Rule</masterLabel>
    <securityOption>EnforceSharingRules</securityOption>
    <sObjectType>Student__c</sObjectType>
    <sortOrder>1</sortOrder>
</DuplicateRule>
